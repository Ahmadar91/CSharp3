<Window x:Class="MultiMediaPlayer.Views.MediaFileTypes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MultiMediaPlayer.Views" xmlns:viewmodels="clr-namespace:MultiMediaPlayer.ViewModels" d:DataContext="{d:DesignInstance Type=viewmodels:DirectoryStructureViewModel}"
        mc:Ignorable="d"
        Title="MediaFileTypes" Height="258" Width="457">

    <GroupBox Header="Image type" Margin="39,38,318,55">
        <StackPanel x:Name="Panel1" Height="73">
            <CheckBox x:Name="JPG" Content=".jpg" HorizontalAlignment="Left" IsEnabled="True" Margin="0,0,0,0" VerticalAlignment="Top"/>
            <CheckBox x:Name="PNG" Content=".png" HorizontalAlignment="Left" IsEnabled="True" Margin="0,20,0,0" VerticalAlignment="Top"/>
        </StackPanel>
    </GroupBox>

</Window>
